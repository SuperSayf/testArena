{
    "testRegex": ["/*.test.js$", "/*.test.jsx$"],
    "collectCoverage": true,
    "coverageReporters": ["lcov"],
    "coverageDirectory": "coverage",
    "coverageThreshold": {
     "global": {
     "branches": 0,
     "functions": 0,
     "lines": 0,
     "statements": 0
     }
    },
    "moduleDirectories": ["node_modules", "src"],
    "moduleNameMapper": {
        "axios": "axios/dist/node/axios.cjs"
      }
   }